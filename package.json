{
  "name": "test",
  "version": "1.1.0",
  "origin": "5.0.0",
  "license": "ISC",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development run-p -l start:*",
    "start:i18n": "bash build.sh",
    "start:webpack": "webpack --config webpack.dev.js",
    "start:eleventy": "eleventy --serve",
    "build": "cross-env NODE_ENV=production run-s -l build:*",
    "build:i18n": "bash build.sh",
    "build:webpack": "webpack --config webpack.prod.js",
    "build:eleventy": "eleventy",
    "staging": "cross-env NODE_ENV=production run-s -l staging:*",
    "staging:i18n": "bash build.sh",
    "staging:webpack": "webpack --config webpack.staging.js",
    "staging:eleventy": "eleventy",
    "analyze": "run-s build analyze:*",
    "analyze:js": "source-map-explorer dist/**/*.js dist/**/*.js.map",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "copy-webpack-plugin": "^6.2.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^7.10.0",
    "eslint-webpack-plugin": "^2.1.0",
    "file-loader": "^6.0.0",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^4.3.0",
    "imagemin-jpeg-recompress": "^7.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "js-yaml": "^3.14.0",
    "lodash.get": "^4.4.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "node-sass-glob-importer": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "source-map-explorer": "^2.4.2",
    "templite": "^1.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  }
}
